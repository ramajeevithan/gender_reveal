{
  "rules": {
    ".read": false,
    ".write": false,
    "votes": {
      ".read": true,
      ".write": "auth != null"
    },
    "status": {
      ".read": true,
      ".write": "auth != null"
    },
    "voters": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    }
  }
}